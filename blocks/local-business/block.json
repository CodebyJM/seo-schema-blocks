{
	"apiVersion": 2,
	"name": "gsb/local-business",
	"title": "SEO Local Business Schema",
	"category": "widgets",
	"icon": "location-alt",
	"description": "Outputs LocalBusiness JSON-LD schema for this page.",
	"keywords": [ "seo", "schema", "json-ld", "local" ],
	"textdomain": "seo-schema-blocks",
	"supports": {
		"html": false,
		"reusable": true
	},
	"attributes": {
		"businessName": {
			"type": "string"
		},
		"businessType": {
			"type": "string",
			"default": "LocalBusiness"
		},
		"url": {
			"type": "string"
		},
		"telephone": {
			"type": "string"
		},
		"priceRange": {
			"type": "string"
		},
		"streetAddress": {
			"type": "string"
		},
		"addressLocality": {
			"type": "string"
		},
		"addressRegion": {
			"type": "string"
		},
		"postalCode": {
			"type": "string"
		},
		"addressCountry": {
			"type": "string",
			"default": "US"
		}
	},
	"editorScript": "gsb-local-business-editor"
}
